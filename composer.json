{
    "name": "app/notifier",
    "description": "Simple php notifier",
    "authors": [
        {
            "name": "Andy",
            "email": "andres.dazai@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "manelpm10/php-value-objects": "dev-master",
        "phpmailer/phpmailer": "dev-master",
        "php-di/php-di": "dev-master",
        "vlucas/phpdotenv": "^5.3",
        "monolog/monolog": "^2.0",
        "league/tactician": "^2.0",
        "symfony/config": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Apps\\": "apps/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "behat/behat": "^3.4",
        "behat/mink": "^1.4",
        "behat/mink-extension": "^2.3"
    }
}
